@prefix : <http://www.semanticweb.org/OntologiaRicercaLavoro#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ns: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix jso: <http://www.semanticweb.org/OntologiaRicercaLavoro#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix wot: <http://xmlns.com/wot/0.1/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix frapo: <http://purl.org/cerif/frapo/> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix schema: <http://www.semanticweb.org/OntologiaRicercaLavoro#schema:> .
@prefix objectrole: <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/OntologiaRicercaLavoro> .

<http://www.semanticweb.org/OntologiaRicercaLavoro> rdf:type owl:Ontology ;
                                                     owl:imports <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl> ,
                                                                 foaf: ;
                                                     dc:creator "Pietro Pettinato" ,
                                                                "Salvatore Latino" ;
                                                     dc:description "Si propone un’ontologia di ricerca/consultazione che permetta collegare chi cerca lavoro con chi offre lavoro. Quindi, che permetta un duplice vantaggio: Per le aziende, trovare il candidato ideale in maniera più semplice, mentre per il singolo lavoratore trovare il lavoro che più gli si addice." ;
                                                     dc:title "Job Search Ontology (JSO)" ;
                                                     vann:preferredNamespacePrefix "jso" ;
                                                     vann:preferredNamespaceUri "http://www.semanticweb.org/OntologiaRicercaLavoro#" ;
                                                     rdfs:label "Job Search Ontology"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#schema:image
schema:image rdf:type owl:AnnotationProperty ;
             rdfs:comment <https://www.nanopress.it/wp-content/uploads/sites/17/2018/02/Immagini-profilo-Facebook.jpg> .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/cerif/frapo/employs
frapo:employs rdf:type owl:ObjectProperty ;
              owl:equivalentProperty jso:haLavoratore ;
              owl:inverseOf jso:lavoraPer ;
              rdfs:domain jso:Azienda ;
              rdfs:range jso:Candidato ;
              rdfs:comment "Un object property che collega un agente che dà lavoro a un agente dipendente, ad esempio una persona o un'organizzazione, che è tipicamente impiegato per svolgere un lavoro specifico in cambio di un pagamento."@it ;
              rdfs:label "frapo:employs"@it .


###  http://purl.org/cerif/frapo/hasCurriculumVitae
frapo:hasCurriculumVitae rdf:type owl:ObjectProperty ;
                         owl:equivalentProperty jso:possiede ;
                         rdfs:domain <http://www.w3.org/2000/10/swap/pim/contact#Person> ;
                         rdfs:range jso:CV ;
                         rdfs:comment "An object property linking a person to an on-line version of that person's curriculum vitae."@it ;
                         rdfs:label "frapo:hasCurriculumVitae"@it .


###  http://purl.org/cerif/frapo/isEmployedBy
frapo:isEmployedBy rdf:type owl:ObjectProperty ;
                   owl:equivalentProperty jso:lavoraPer ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain jso:Candidato ;
                   rdfs:range jso:Azienda ;
                   rdfs:comment "Una object property che collega un agente dipendente, tipicamente una persona o un'organizzazione, a un agente che dà lavoro."@it ;
                   rdfs:label "frapo:isEmployedBy"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#CV_è_di
jso:CV_è_di rdf:type owl:ObjectProperty ;
            owl:inverseOf jso:possiede ;
            rdfs:comment "Indica a chi appartiene un CV"@it ;
            rdfs:label "CV_è_di"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#annuncioConsigliato
jso:annuncioConsigliato rdf:type owl:ObjectProperty ;
                        rdfs:domain jso:Candidato ;
                        rdfs:range jso:Annuncio ;
                        rdfs:comment "Un annuncio consigliato per un certo candiddato"@it ;
                        rdfs:label "annuncioConsigliato"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#contiene
jso:contiene rdf:type owl:ObjectProperty ;
             owl:inverseOf jso:èContenutoIn ;
             rdf:type owl:TransitiveProperty ;
             rdfs:domain jso:CV ;
             rdfs:range jso:Campi_CV ;
             rdfs:comment "contiene qualcosa"@it ;
             rdfs:label "contiene"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#contrattoProposto
jso:contrattoProposto rdf:type owl:ObjectProperty ;
                      rdfs:domain jso:Annuncio ;
                      rdfs:range jso:Contratto ;
                      owl:propertyDisjointWith jso:haContratto ;
                      rdfs:comment "contratto proposto per un'offerta di lavoro"@it ;
                      rdfs:label "contrattoProposto"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#ePubblicato
jso:ePubblicato rdf:type owl:ObjectProperty ;
                owl:inverseOf jso:pubblica ;
                rdfs:domain jso:Annuncio ;
                rdfs:range jso:Azienda ;
                rdfs:comment "pubblicazione di un annuncio"@it ;
                rdfs:label "ePubblicato"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#eUnRuoloDi
jso:eUnRuoloDi rdf:type owl:ObjectProperty ;
               owl:inverseOf jso:haRuolo ;
               rdfs:domain jso:FunzioniOccupate ;
               rdfs:range owl:Thing ;
               rdfs:comment "è ruolo di qualcosa"@it ;
               rdfs:label "eUnRuoloDi"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#gestisce
jso:gestisce rdf:type owl:ObjectProperty ;
             owl:inverseOf jso:èGestitaDa ;
             rdfs:domain jso:Candidato ;
             rdfs:range jso:Azienda ;
             rdfs:comment "indica chi gestisce un'azienda"@it ;
             rdfs:label "gestisce"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haCampo
jso:haCampo rdf:type owl:ObjectProperty ;
            rdfs:domain jso:Campi_CV ;
            rdfs:comment "campi di un CV"@it ;
            rdfs:label "haCampo"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haCompetenze
jso:haCompetenze rdf:type owl:ObjectProperty ;
                 rdfs:domain jso:Competenze ;
                 rdfs:range jso:Competenze ;
                 rdfs:comment "possiede una certa competenza"@it ;
                 rdfs:label "haCompetenze"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haCompetenzeRichieste
jso:haCompetenzeRichieste rdf:type owl:ObjectProperty ;
                          rdfs:domain jso:Annuncio ;
                          rdfs:range jso:Competenze ;
                          rdfs:comment "possiede delle competenze richieste in un annuncio"@it ;
                          rdfs:label "haCompetenzeRichieste"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haContratto
jso:haContratto rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain jso:Candidato ;
                rdfs:range jso:Contratto ;
                rdfs:comment "indica il contratto di lavoro"@it ;
                rdfs:label "haContratto"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haLavoratore
jso:haLavoratore rdf:type owl:ObjectProperty ;
                 owl:inverseOf jso:lavoraPer ;
                 rdfs:domain jso:Azienda ;
                 rdfs:range jso:Candidato ;
                 rdfs:comment "lavoratore di un'azienda"@it ;
                 rdfs:label "haLavoratore"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haRuolo
jso:haRuolo rdf:type owl:ObjectProperty ;
            rdfs:domain jso:FunzioniOccupate ;
            rdfs:range owl:Thing ;
            rdfs:comment "il ruolo di qualcosa"@it ;
            rdfs:label "haRuolo"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haSedePrincipaleIn
jso:haSedePrincipaleIn rdf:type owl:ObjectProperty ;
                       rdfs:domain jso:Azienda ;
                       rdfs:range jso:Nazione ;
                       rdfs:comment "indica la sede principale di un'azienda"@it ;
                       rdfs:label "haSedePrincipaleIn"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haSettoreAnnuncio
jso:haSettoreAnnuncio rdf:type owl:ObjectProperty ;
                      rdfs:domain jso:Annuncio ;
                      rdfs:range jso:Settore ;
                      owl:propertyChainAxiom ( jso:ePubblicato
                                               jso:haSettoreAzienda
                                             ) ;
                      rdfs:comment "indica il settore di un annuncio"@it ;
                      rdfs:label "haSettoreAnnuncio"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haSettoreAzienda
jso:haSettoreAzienda rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain jso:Azienda ;
                     rdfs:range jso:Settore ;
                     rdfs:comment "indica il settore in cui opera un'azienda"@it ;
                     rdfs:label "haSettoreAzienda"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haSettoreDiInteresse
jso:haSettoreDiInteresse rdf:type owl:ObjectProperty ,
                                  owl:TransitiveProperty ;
                         rdfs:domain jso:Candidato ;
                         rdfs:range jso:Settore ;
                         owl:propertyChainAxiom ( jso:lavoraPer
                                                  jso:haSettoreAzienda
                                                ) ,
                                                ( jso:possiede
                                                  jso:contiene
                                                  jso:haCampo
                                                  jso:haSettoreStudi
                                                ) ;
                         rdfs:comment "indica il settore di interesse di un candidato"@it ;
                         rdfs:label "haSettoreDiInteresse"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haSettoreStudi
jso:haSettoreStudi rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain jso:StudiUniversitari ;
                   rdfs:range jso:Settore ;
                   rdfs:comment "indica il settore di studi"@it ;
                   rdfs:label "haSettoreStudi"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#lavoraNelSettore
jso:lavoraNelSettore rdf:type owl:ObjectProperty ;
                     owl:equivalentProperty foaf:topic_interest ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdfs:domain jso:Candidato ;
                     rdfs:range jso:Settore ;
                     owl:propertyChainAxiom ( jso:lavoraPer
                                              jso:haSettoreAzienda
                                            ) ;
                     rdfs:comment "indica il settore in cui lavora un candidato"@it ;
                     rdfs:label "lavoraNelSettore"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#lavoraPer
jso:lavoraPer rdf:type owl:ObjectProperty ;
              rdfs:domain jso:Candidato ;
              rdfs:range jso:Azienda ;
              rdfs:comment "indica l'azienda per cui lavora un candidato"@it ;
              rdfs:label "lavoraPer"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#possiede
jso:possiede rdf:type owl:ObjectProperty ;
             rdfs:domain jso:Candidato ;
             rdfs:range jso:CV ;
             rdfs:comment "a chi appartiene il CV"@it ;
             rdfs:label "possiede"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#pubblica
jso:pubblica rdf:type owl:ObjectProperty ;
             owl:equivalentProperty foaf:made ;
             rdf:type owl:InverseFunctionalProperty ;
             rdfs:domain jso:Azienda ;
             rdfs:range jso:Annuncio ;
             rdfs:comment "indica chi pubblica un annuncio"@it ;
             rdfs:label "pubblica"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#èContenutoIn
jso:èContenutoIn rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:comment "qualcosa è contenuta in qualcosa"@it ;
                 rdfs:label "èContenutoIn"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#èGestitaDa
jso:èGestitaDa rdf:type owl:ObjectProperty ;
               rdfs:domain jso:Azienda ;
               rdfs:range jso:Candidato ;
               rdfs:comment "da chi è gestita un'azienda"@it ;
               rdfs:label "èGestitaDa"@it .


###  http://xmlns.com/foaf/0.1/haPersonalmailbox
foaf:haPersonalmailbox rdfs:domain jso:InfoPersonali .


###  http://xmlns.com/foaf/0.1/img
foaf:img rdfs:domain jso:InfoPersonali .


###  http://xmlns.com/foaf/0.1/made

###  http://xmlns.com/foaf/0.1/topic_interest
foaf:topic_interest rdf:type owl:ObjectProperty ;
                    rdfs:domain foaf:Agent ;
                    rdfs:range owl:Thing ;
                    rdfs:comment """A thing of interest to this person.

La proprietà topic_interest collega un agente a una cosa a cui è interessato. A differenza dell'argomento, non è diretta attraverso un documento, ma collega direttamente la cosa."""@it ;
                    rdfs:label "topic_interest"@it .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/cerif/frapo/hasCountry
frapo:hasCountry rdf:type owl:DatatypeProperty ;
                 rdfs:comment "A data property specifying a country, for example as the location of an event."@it ;
                 rdfs:label "frapo:hasCountry"@it .


###  http://purl.org/cerif/frapo/hasJobTitle
frapo:hasJobTitle rdf:type owl:DatatypeProperty ;
                  rdfs:comment """A data property permitting specification of the job title held by a person, such as might appear next to the name in a directory or on a conference name badge. For example, the person's specific Job Title might be 'Linacre Professor of Zoology' or 'Regius Professor of Moral Philosophy'.

This job title properly refers to the role held by that person (e.g. 'Head of Department')"""@it ;
                  rdfs:label "frapo:hasJobTitle"@it .


###  http://purl.org/cerif/frapo/hasPhoneNumber
frapo:hasPhoneNumber rdf:type owl:DatatypeProperty ;
                     owl:equivalentProperty jso:recapitoTelefonico ;
                     rdfs:comment "A data property identifying a telephone number for an agent."@it ;
                     rdfs:label "frapo:hasPhoneNumber"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#anniCarriera
jso:anniCarriera rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "Il numero di anni di carriera"@it ;
                 rdfs:label "anniCarriera"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#benefits
jso:benefits rdf:type owl:DatatypeProperty ;
             rdfs:domain jso:Annuncio ;
             rdfs:range xsd:string ;
             rdfs:comment "i benefits di un annuncio lavorativo"@it ;
             rdfs:label "benefits"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#cercaLavoro
jso:cercaLavoro rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain jso:Candidato ;
                rdfs:range xsd:boolean ;
                rdfs:comment "indica se un candidato cerca lavoro opure no"@it ;
                rdfs:label "cercaLavoro"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#competenzeDettagli
jso:competenzeDettagli rdf:type owl:DatatypeProperty ;
                       rdfs:domain jso:Competenze ;
                       rdfs:comment "mostra i dattegli di una competenza"@it ;
                       rdfs:label "competenzeDettagli"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#competenzeRichiesteDettagli
jso:competenzeRichiesteDettagli rdf:type owl:DatatypeProperty ;
                                rdfs:domain jso:Annuncio ;
                                rdfs:range xsd:string ;
                                rdfs:comment "mostra i dettagli di una competenza richiesta"@it ;
                                rdfs:label "competenzeRichiesteDettagli"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#dataNascita
jso:dataNascita rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain jso:InfoPersonali ;
                rdfs:range xsd:date ;
                rdfs:comment "data di nascita di un individuo"@it ;
                rdfs:label "dataNascita"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#descrizione
jso:descrizione rdf:type owl:DatatypeProperty ;
                rdfs:domain jso:Annuncio ;
                rdfs:range xsd:string ;
                rdfs:comment "descrizione di un annuncio"@it ;
                rdfs:label "descrizione"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#descrizioneAzienda
jso:descrizioneAzienda rdf:type owl:DatatypeProperty ;
                       rdfs:domain jso:Azienda ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Descrizione di un'azienda"@it ;
                       rdfs:label "descrizioneAzienda"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#email
jso:email rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string ;
          rdfs:comment "indirizzo di posta elettronica"@it ;
          rdfs:label "email"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#genere
jso:genere rdf:type owl:DatatypeProperty ;
           rdfs:domain jso:InfoPersonali ;
           rdfs:range xsd:string ;
           rdfs:comment "il genere di un agente"@it ;
           rdfs:label "genere"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#haOccupatoFunzione
jso:haOccupatoFunzione rdf:type owl:DatatypeProperty ;
                       rdfs:range [ rdf:type rdfs:Datatype ;
                                    owl:oneOf [ rdf:type rdf:List ;
                                                rdf:first "Developer" ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first "Manager" ;
                                                           rdf:rest [ rdf:type rdf:List ;
                                                                      rdf:first "altro" ;
                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                 rdf:first "assistente" ;
                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                            rdf:first "insegnante" ;
                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                       rdf:first "operaio" ;
                                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                                  rdf:first "titotale attività" ;
                                                                                                                  rdf:rest rdf:nil
                                                                                                                ]
                                                                                                     ]
                                                                                          ]
                                                                               ]
                                                                    ]
                                                         ]
                                              ]
                                  ] ;
                       rdfs:comment "funzione lavorativa che ha occupato un agente"@it ;
                       rdfs:label "haOccupatoFunzione"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#indirizzo
jso:indirizzo rdf:type owl:DatatypeProperty ;
              rdfs:domain jso:InfoPersonali ;
              rdfs:range xsd:string ;
              rdfs:comment "indirizzo di un luogo"@it ;
              rdfs:label "indirizzo"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#luogo
jso:luogo rdf:type owl:DatatypeProperty ;
          rdfs:domain jso:Annuncio ;
          rdfs:range xsd:string ;
          rdfs:comment "luogo geografico"@it ;
          rdfs:label "luogo"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#nazionalita
jso:nazionalita rdf:type owl:DatatypeProperty ;
                rdfs:domain jso:InfoPersonali ;
                rdfs:range xsd:string ;
                rdfs:comment "nazione di nascita di un agente"@it ;
                rdfs:label "nazionalita"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#nome
jso:nome rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:string ;
         rdfs:comment "nome di un individuo"@it ;
         rdfs:label "nome"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#nomeAzienda
jso:nomeAzienda rdf:type owl:DatatypeProperty ;
                rdfs:domain jso:Azienda ;
                rdfs:range xsd:string ;
                rdfs:comment "nome di un'azienda"@it ;
                rdfs:label "nomeAzienda"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#numeroSedi
jso:numeroSedi rdf:type owl:DatatypeProperty ;
               rdfs:domain jso:Azienda ;
               rdfs:range xsd:integer ;
               rdfs:comment "nomero delle sedi di un'azienda"@it ;
               rdfs:label "numeroSedi"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#oreDiLavoro
jso:oreDiLavoro rdf:type owl:DatatypeProperty ;
                rdfs:domain jso:Annuncio ;
                rdfs:range xsd:integer ;
                rdfs:comment "ore di lavoro previste in un annuncio"@it ;
                rdfs:label "oreDiLavoro"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#professione
jso:professione rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain jso:InfoPersonali ;
                rdfs:range xsd:string ;
                rdfs:comment "professione di un agente"@it ;
                rdfs:label "professione"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#recapitoTelefonico
jso:recapitoTelefonico rdf:type owl:DatatypeProperty ;
                       rdfs:range xsd:string ;
                       rdfs:comment "numero di telefono di un individuo"@it ;
                       rdfs:label "recapitoTelefonico"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#salarioMensile
jso:salarioMensile rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:float ;
                   rdfs:comment "salario mensile previsto in un'offerta di lavoro"@it ;
                   rdfs:label "salarioMensile"@it .


###  http://www.semanticweb.org/OntologiaRicercaLavoro#viveIn
jso:viveIn rdf:type owl:DatatypeProperty ;
           rdfs:domain jso:InfoPersonali ;
           rdfs:range xsd:string ;
           rdfs:comment "luogo in cui vive un individuo"@it ;
           rdfs:label "viveIn"@it .


